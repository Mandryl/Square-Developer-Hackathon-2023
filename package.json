{
  "name": "coffee-blend-app",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node server/app.js",
    "serve": "vite build && nodemon server/app.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "seed": "node ./bin/script/seed-catalog.js generate",
    "clear": "node ./bin/script/seed-catalog.js clear",
    "help": "node ./bin/script/seed-catalog.js --help"
  },
  "dependencies": {
    "apexcharts": "^3.40.0",
    "axios": "^1.4.0",
    "dotenv": "^16.1.3",
    "element-plus": "^2.3.6",
    "express": "^4.18.2",
    "log4js": "^6.9.1",
    "morgan": "^1.10.0",
    "openai": "^3.2.1",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "pinia": "^2.0.36",
    "shuffle-array": "^1.0.1",
    "square": "^27.0.0",
    "uuid": "^9.0.0",
    "vue": "^3.3.2",
    "vue-router": "^4.2.0",
    "vue3-apexcharts": "^1.4.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "eslint": "^8.39.0",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-vue": "^9.11.0",
    "prettier": "^2.8.8",
    "vite": "^4.3.5"
  }
}
